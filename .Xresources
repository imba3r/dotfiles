! URxvt Settings
URxvt*font: xft:FiraMono:style=Regular:size=15
URxvt*boldFont: xft:FiraMono:style=Bold:size=15
URxvt.scrollBar: false
URxvt.perl-ext-common: url-select,clipboard
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.inheritPixmap: true
URxvt.transparent: true
URxvt.shading: 10
URxvt.letterSpace: -1

! Xft Settings
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintslight

! Rofi Settings
rofi.font: Fira Mono Regular 14
rofi.bw: 2
rofi.color-enabled: true
!                  'bg'   , 'bd'   , 'sep'
rofi.color-window: #222D32, #1A2327, #1A2327
! State:           'bg'   , 'fg'   , 'bgalt', 'hlbg' , 'hlfg'
rofi.color-normal: #222D32, #FAFBFC, #1A2327, #00BCD4, #FAFBFC
rofi.color-active: #222D32, #53A6A6, #1A2327, #287373, #FAFBFC
rofi.color-urgent: #222D32, #CF4F88, #1A2327, #8A2F58, #FAFBFC